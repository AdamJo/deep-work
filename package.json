{
  "name": "deep-work",
  "version": "0.0.1",
  "private": true,
  "license": "MIT",
  "repository": "adamjo/deep-work",
  "author": {
    "name": "Adam Johannesmeyer",
    "email": "adam.johannesmeyer@gmail.com"
  },
  "scripts": {
    "dev": "npm run env:dev -- webpack-dev-server --progress",
    "start": "npm run dev",
    "start:prod": "npm run prod",
    "prod": "npm run build && serve dist -s",
    "prod:server": "serve dist -s",
    "env:dev": "cross-env NODE_ENV=development",
    "env:prod": "cross-env NODE_ENV=production",
    "test": "jest",
    "test:watch": "cross-env NODE_ENV=test jest --watch",
    "coverage": "npm test -- --coverage --watch",
    "coverage:only": "npm test -- --coverage",
    "postcoverage": "opn coverage/lcov-report/index.html",
    "lint": "eslint src src-clean test .storybook webpack.config.js",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "build": "npm run env:prod -- webpack --progress",
    "create": "yo arc:component --ours",
    "lightnouse": "lighthouse https://deepwork.com --output-path=./lighthouse/lighthouse.html --output html",
    "prettier": "prettier --write --single-quote --trailing-comma=all \"src/**/*.js\"",
    "deploy": "firebase deploy"
  },
  "jest": {
    "moduleDirectories": [
      "src",
      "node_modules"
    ],
    "moduleNameMapper": {
      "^.+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/test/fileMock.js",
      "^components$": "<rootDir>/test/componentsMock.js"
    }
  },
  "devDependencies": {
    "@kadira/storybook": "^2.35.2",
    "@webpack-blocks/dev-server2": "^0.4.0",
    "@webpack-blocks/webpack2": "^0.4.0",
    "babel-core": "^6.24.0",
    "babel-eslint": "^7.2.1",
    "babel-jest": "^19.0.0",
    "babel-loader": "^6.4.1",
    "babel-plugin-module-resolver": "^2.7.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.3.0",
    "babel-preset-env": "^1.3.2",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-1": "^6.16.0",
    "clean-webpack-plugin": "^0.1.16",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^4.0.0",
    "enzyme": "~2.8.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.0.0",
    "eslint-import-resolver-webpack": "^0.8.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-no-unused-vars-rest": "^1.0.6",
    "eslint-plugin-react": "^6.10.3",
    "file-loader": "^0.11.1",
    "happypack": "^3.0.3",
    "html-webpack-plugin": "^2.26.0",
    "jest-cli": "^19.0.0",
    "lodash-webpack-plugin": "^0.11.2",
    "offline-plugin": "^4.6.2",
    "opn-cli": "^3.1.0",
    "raw-loader": "^0.5.1",
    "rc-slider": "^6.3.1",
    "react-addons-test-utils": "^15.4.1",
    "redux-logger": "^3.0.1",
    "rimraf": "^2.6.1",
    "serve": "^5.1.2",
    "shortid": "^2.2.8",
    "string-replace-webpack-plugin": "^0.1.3",
    "url-loader": "^0.5.7",
    "webpack": "^2.3.3",
    "webpack-bundle-analyzer": "^2.3.1",
    "webpack-bundle-size-analyzer": "^2.6.0",
    "webpack-dev-server": "^2.4.2",
    "webpack-md5-hash": "^0.0.5"
  },
  "dependencies": {
    "babel-regenerator-runtime": "^6.5.0",
    "firebase": "^3.7.5",
    "json2csv": "^3.7.3",
    "react": "~15.4.1",
    "react-dom": "~15.4.1",
    "react-modal": "^1.7.3",
    "react-redux": "^5.0.3",
    "redux": "^3.6.0",
    "redux-saga": "^0.14.4",
    "styled-components": "^1.2.1",
    "styled-theme": "^0.3.0",
    "styled-tools": "^0.1.2"
  }
}
